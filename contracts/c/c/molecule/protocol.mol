array Hash [byte; 32];
vector String <byte>;
option StringOpt (String);
vector StringVec <String>;

// represent global data
table Flag_0 {
    project_id: Hash,
}

// represent personal data
table Flag_1 {
    project_id: Hash,
}

// represent contract request call
table Flag_2 {
    project_id: Hash,
}

// represent simplified cell format
table Cell {
    owner_lockscript: String,
    data: StringOpt,
}

vector CellVec <Cell>;

// describe request information
table Request {
    cells: CellVec,
    function_call: String,
    function_celldeps: StringVec,
    floating_lockscripts: StringVec,
}
