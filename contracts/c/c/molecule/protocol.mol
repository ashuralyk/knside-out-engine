array Hash [byte; 32];
vector String <byte>;
option StringOpt (String);
vector StringVec <String>;
option StringVecOpt (StringVec);

// represent global data
table Flag_0 {
    project_id: Hash,
}

// represent personal data
table Flag_1 {
    project_id: Hash,
}

// represent contract request call
table Flag_2 {
    function_call: String,
    caller_lockscript: String,
    recipient_lockscript: StringOpt,
    personal_celldeps: StringVecOpt,
}
